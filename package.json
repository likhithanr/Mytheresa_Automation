{
  "name": "mytheresa_automation",
  "version": "1.0.0",
  "description": "automate mytheresa ui using cypress framework",
  "main": "index.js",
  "scripts": {
    "cy:run:prod": "cypress run --env ENV=\"production\"",
    "cy:run:stg": "cypress run --env ENV=\"staging\"",
    "cy:run:test": "cypress run --env ENV=\"test\"",
    "cy:run:loc": "cypress run --env ENV=\"local\"",
    "cy:run:prod:chrome":"cypress run --env ENV=\"production\" --browser chrome",
    "cy:run:prod:FF":"cypress run --env ENV=\"production\" --browser Firefox",
    "downloadOpenPulls":"cypress run --spec cypress/integration/testScripts/GetOpenGitPulls.js"
  },
  "author": "Likhitha N R",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^9.7.0",
    "cypress-mochawesome-reporter": "^3.2.0",
    "install": "^0.13.0"
  },
  "dependencies": {
    "aggregate-error": "^3.1.0",
    "performance-now": "^2.1.0",
    "pify": "^2.3.0",
    "pretty-bytes": "^5.6.0",
    "proxy-from-env": "^1.0.0"
  }
}
